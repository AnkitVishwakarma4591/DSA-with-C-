# include <iostream>
using namespace std;

int main(){
    int a,b;
    cout<<"Enter a :";
    cin>>a;
    cout<<"Enter b :";
    cin>>b;

    cout<<"sum = "<<a+b<<endl;
    cout<<"Difference = "<<a-b<<endl;
    cout<<"Product = "<<a*b<<endl;
    cout<<"Divide = "<<a/b<<endl;
    cout<<"Reminder = "<<a%b<<endl;
    
    return 0;
}